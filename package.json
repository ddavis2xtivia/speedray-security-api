{
  "name": "@speedray/typescript-fetch-api",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "main": "./dist/api.js",
  "browser": "./dist/api.js",
  "typings": "./dist/api.d.ts",
  "dependencies": {
    "core-js": "^2.4.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "scripts": {
